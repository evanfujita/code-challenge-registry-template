<h1> Create Registry </h1>

<%= form_for(@registry) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <%= f.collection_select :coordinator_id, Coordinator.all, :id, :name, prompt: true %>
    <%= f.submit %>
<% end %>
